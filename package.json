{
  "name": "automount",
  "version": "0.0.17",
  "description": "A simple framework to easily mount React components on a dynamic webpage",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedelia/automount.git"
  },
  "keywords": [
    "react",
    "mount",
    "render",
    "dom"
  ],
  "author": "Joe d'Elia",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/joedelia/automount/issues"
  },
  "homepage": "https://github.com/joedelia/automount#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "16.x.x",
    "react-dom": "16.x.x"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "grunt": "^1.4.1",
    "grunt-babel": "^8.0.0",
    "grunt-bump": "^0.8.0",
    "grunt-notify": "^0.4.5",
    "load-grunt-tasks": "^3.5.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "xo": "^0.44.0"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none",
          "varsIgnorePattern": "^_"
        }
      ],
      "one-var": [
        "warn",
        {
          "uninitialized": "always"
        }
      ],
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-new": 0,
      "no-prototype-builtins": 0,
      "no-loop-func": 0,
      "no-alert": 0,
      "indent": [
        "error",
        2
      ]
    },
    "ignores": [
      "Gruntfile.js"
    ]
  }
}
